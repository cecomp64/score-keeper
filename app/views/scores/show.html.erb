<div class="page-header">
  <%= link_to scores_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_score_path(@score), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show score</h1>
</div>

<dl class="dl-horizontal">
  <dt>Round:</dt>
  <dd><%= @score.round %></dd>

  <dt>Player:</dt>
  <dd><%= @score.player_id %></dd>

  <dt>Game:</dt>
  <dd><%= @score.game_id %></dd>

  <dt>Score:</dt>
  <dd><%= @score.score %></dd>

</dl>
